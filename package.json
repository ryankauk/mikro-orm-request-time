{
  "name": "mikro-orm-request-time",
  "version": "0.0.1",
  "description": "show execution time of mikro orm parallel requests",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Ryan Kauk",
  "license": "ISC",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev src/index.ts --ignore-watch node_modules --respawn --transpile-only",
    "ts-node": "ts-node --transpile-only",
    "request": "yarn ts-node ./src/req",
    "stats": "npx autocannon"
  },
  "dependencies": {
    "@mikro-orm/core": "^4.2.3",
    "@mikro-orm/mongo-highlighter": "^1.0.0",
    "@mikro-orm/mongodb": "^4.2.3",
    "@mikro-orm/reflection": "^4.2.3",
    "ansi-colors": "^4.1.1",
    "debug": "^4.2.0",
    "express": "^4.17.1",
    "express-promise-router": "^4.0.1",
    "mongoose": "^5.11.10",
    "mongoose-simple-random": "^0.4.1"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.14",
    "@types/node-fetch": "^2.5.7",
    "@types/uuid": "^8.3.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "node-fetch": "^2.6.1",
    "prettier": "^2.2.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.0.5"
  },
  "files": [
    "dist"
  ]
}
